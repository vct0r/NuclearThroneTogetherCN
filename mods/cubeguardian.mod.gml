// CUBE GUARDIANS //
#define init
// DEFINE SPRITES //
global.spr_cube_idle = sprite_add_base64("", 6, 30, 30);
global.spr_cube_hurt = sprite_add_base64("", 3, 30, 30);
global.spr_cube_dead = sprite_add_base64("", 9, 30, 30);
global.spr_cube_fire = sprite_add_base64("", 4, 40, 70);
global.spr_cube_strt = sprite_add_base64("", 19, 40, 70);
global.spr_cube_end = sprite_add_base64("", 15, 40, 70);

#define step
with(DogGuardian)
{
	if("cube_processed" not in self)
	{
		if(random(15) < 1)
		{
			with(instance_create(x,y,CustomEnemy))
			{
				spr_idle = global.spr_cube_idle;
				spr_walk = spr_idle;
				spr_hurt = global.spr_cube_hurt;
				spr_dead = global.spr_cube_dead;
				spr_fire = global.spr_cube_fire;
				spr_strt = global.spr_cube_strt;
				spr_end = global.spr_cube_end;
				
				sprite_index = spr_idle;
				mask_index = other.mask_index;
				spr_shadow = other.spr_shadow;
				
				snd_hurt = other.snd_hurt;
				snd_dead = other.snd_dead;
				
				my_health = other.my_health * 1.20;
				lsthealth = my_health; // ODD WORKAROUND FOR HURT SPRITE
				maxhealth = my_health;
				
				
				raddrop = other.raddrop * 1.60;
				
				on_step = script_ref_create(cube_step);
				
				on_draw = script_ref_create(cube_draw);
				
				direction = random(360);
				gunangle = random(360);
				walk = 0;
				meleedamage = 4;
				depth -= 4;
				right = choose(1,-1);
				react_alarm = 60 + random(20);
			}
			
			instance_delete(self);
		}
		else
		{
			cube_processed = 1;
		}
	}
}

#define cube_step
if (react_alarm && !--react_alarm) {
	react_alarm = 60 + random(20);
    cube_alarm();
}

if speed > 2
speed = 2;

if image_index >= image_number - 1
{
	if spr_idle = spr_strt spr_idle = spr_fire; 
	if spr_idle = spr_end{ spr_idle = global.spr_cube_idle; sprite_index = spr_idle; }
	if sprite_index = spr_hurt{ lsthealth = my_health; sprite_index = spr_idle;  }
}

if spr_idle != spr_strt and spr_idle != spr_fire and spr_idle != spr_end
{	
	if walk > 0
	{
		walk -= 1;
		motion_add(direction, 2);
	}
	else
	{
		motion_add(direction, 1);
	}
}
else
{
	speed = 0;
	sprite_index = spr_idle;
	
	if spr_idle = spr_fire
	{
		// COPIED FROM INSPECTOR CODE //
		with Player
		{if point_distance(x,y,other.x,other.y) < 160
		{if place_free(x+lengthdir_x(3,point_direction(x,y,other.x,other.y)),y)
		x += lengthdir_x(3,point_direction(x,y,other.x,other.y))
		if place_free(x,y+lengthdir_y(3,point_direction(x,y,other.x,other.y)))
		y += lengthdir_y(3,point_direction(x,y,other.x,other.y))}}
		
		with instance_create(x+lengthdir_x(32+random(8), point_direction(target.x,target.y,x,y)+180+random(30)-15),y+lengthdir_y(32+random(8), point_direction(target.x,target.y,x,y)+180+random(30)-15), Dust)
		{
			motion_add(point_direction(x,y,other.x,other.y), 3 + random(5))
			depth = other.depth - 1;
		}
		
		if target.x < x
		right = -1;
		else
		right = 1;
	}
}

if spr_idle = global.spr_cube_idle and lsthealth != my_health
{
	sprite_index = spr_hurt;
}

#define cube_alarm
var target = instance_nearest(x,y,Player);

if spr_idle = spr_fire
{
	spr_idle = spr_end;
	image_index = 0;
}

right = choose(1,-1);

if target > 0
{
	gunangle = point_direction(x,y,target.x,target.y);
	
	if collision_line(x,y,target.x,target.y,Wall,0,0) < 0
	{
		if point_distance(x,y,target.x,target.y) < 160 and random(3) < 1 and spr_idle != spr_end
		{
			spr_idle = spr_strt;
			image_index = 0;
			react_alarm = 120;
			
			if target.x < x
			right = -1;
			else
			right = 1;
		}
		else
		{
			walk = 30;
			direction = random(360);
			react_alarm = 20 + random(20);
		}
	}
	else
	{
		walk = 30;
		direction = random(360);
		react_alarm = 20 + random(20);
	}

}
else
{
	walk = 10;
	direction = random(360);
}

#define cube_draw
draw_sprite_ext(sprite_index,-1,x,y,image_xscale*right,image_yscale,image_angle,image_blend,image_alpha);